@{
    Layout = "~/Views/Shared/_LoggedInHeader.cshtml";
}
<link href="~/Content/css/preloader.css" rel="stylesheet" />
<script src="~/Scripts/jquery-2.1.3.js"></script>
<script src="~/Scripts/moreSearch.js"></script>
<div id="preloader_1">
    <span></span>
    <span></span>
    <span></span>
    <span></span>
    <span></span>
</div>
<div id="show"  style="margin-left: 200px; margin-right: 200px;display: none">
   

    <div style="float: left">
        <h2>More People</h2>
        <div class="personMore">

        </div>
    </div>

    <div style="float: right">
        <h2>More Programs</h2>
        <div class="programsMore">


        </div>
    </div>
    <div style="clear: both;">
        <button style="margin-top: 20px; width: 100%" id="getMoreResult">GET MORE SEARCHES</button>
    </div>
</div>

<script>
    setTimeout(function () {
         $('#show').show();
        getMoreSearchData();
       
        
    },250);
   
    $(document).ready(function () {
        $('#getMoreResult').on('click', function () {
            $('#show').hide();
            $('#preloader_1').show();
            setTimeout(function () {
            $('#show').show();
            getMoreSearchData();
            }, 250);
          

        });
    });

</script>